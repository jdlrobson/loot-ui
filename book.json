{
  "//": "Use 2.5.2 until github.com/GitbookIO/gitbook/issues/1029 clarifed or resolved",
  "gitbook": "2.5.2",
  "structure": {
    "summary": "docs/SUMMARY.md"
  },
  "plugins": [
    "edit-link",
    "image-captions",
    "anchors",
    "github"
  ],
  "pluginsConfig": {
    "edit-link": {
      "base": "https://github.com/joakin/loot-ui/tree/master",
      "label": "Edit This Page"
    },
    "image-captions": {
      "caption": "_CAPTION_"
    },
    "github": {
      "url": "https://github.com/joakin/loot-ui"
    }
  }
}
